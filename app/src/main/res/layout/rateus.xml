<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="300dp"
    android:layout_height="300dp"
    android:layout_gravity="center"
    android:background="#1AD68585">

    <FrameLayout
        android:id="@+id/frameLayout3b"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#39B3AEAE"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView36b"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="RATE US"
            android:textSize="17sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dialogclsb"
            android:layout_width="50dp"
            android:layout_height="50dp"

            android:layout_gravity="right"
            android:gravity="center"
            android:text="X"
            android:textSize="22sp"
            android:textStyle="bold" />
    </FrameLayout>

    <RatingBar
        android:id="@+id/ratingBar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:clickable="false"
        android:indeterminate="false"
        android:numStars="5"
        android:rating="5"
        android:stepSize="1"
        android:theme="@style/rat"
        app:layout_constraintBottom_toTopOf="@+id/buttondb"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout3b" />

    <Button
        android:id="@+id/buttondb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Submit"
        app:backgroundTint="#878585"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ratingBar2" />

</androidx.constraintlayout.widget.ConstraintLayout>